      private: |
        -----BEGIN RSA PRIVATE KEY-----
        a private ssh key goes here
        -----END RSA PRIVATE KEY-----
      public: >
        ssh-rsa a public ssh key goes here cideploy@bastion
    nodepool:
      private: |
        -----BEGIN RSA PRIVATE KEY-----
        a private ssh key goes here
        -----END RSA PRIVATE KEY-----
      public: >
        ssh-rsa a public ssh key goes here nodepool@nodepoop
    zuul:
      private: |
        -----BEGIN RSA PRIVATE KEY-----
        a private ssh key goes here
        -----END RSA PRIVATE KEY-----
      public: >
        ssh-rsa a public ssh key goes here zuul@zuul
